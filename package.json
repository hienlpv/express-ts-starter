{
    "name": "node-express-ts-starter",
    "version": "1.0.0",
    "description": "Starter template for developing with node, express and typescript. Includes configuration for prettier, eslint and testing with jasmine",
    "main": "index.js",
    "scripts": {
        "start": "nodemon -L ./src/index.ts",
        "build": "npx tsc",
        "lint": "eslint . --ext .ts",
        "prettier": "prettier . --write",
        "jasmine": "jasmine",
        "test": "npm run build && npm run jasmine"
    },
    "author": "Alfonsina Lizardo",
    "license": "ISC",
    "dependencies": {
        "express": "^4.18.2",
        "jasmine": "^5.1.0",
        "jasmine-spec-reporter": "^7.0.0",
        "supertest": "^6.3.3"
    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/jasmine": "^4.3.5",
        "@types/node": "^20.5.9",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^6.5.0",
        "@typescript-eslint/parser": "^6.5.0",
        "eslint": "^8.48.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^17.1.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-prettier": "^5.0.0",
        "nodemon": "^3.0.1",
        "prettier": "^3.0.3",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2"
    }
}
